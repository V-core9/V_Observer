    "glob-parent": ">=5.1.2",
    "image-to-base64": "^2.2.0",
    "node-notifier": "^10.0.0",
    "node-os-utils": "^1.3.6",
    "openweather-apis": "^4.4.2",
    "screenshot-desktop": "^1.12.7",
    "speedtest-net": "^2.2.0",
    "svg-to-png": "^4.0.0",
